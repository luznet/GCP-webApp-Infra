# Required variables for PROD environment
db_user      = "webappuser"
db_version   = "POSTGRES_14"
environment = "prod"
project_id  = "your-prod-gcp-project-id"
db_password = "your-prod-db-password"
lb_domain   = "your-domain.com"
region      = "europe-west1"
zone        = "europe-west1-b"

owner        = "prod-owner"
project_name = "webapp-prod"
used_by      = "prod-team"
owner_email  = "prod-owner@example.com"

# Cloud SQL backup config (PROD example)
backup_enabled = true
point_in_time_recovery_enabled = true
transaction_log_retention_days = 7
